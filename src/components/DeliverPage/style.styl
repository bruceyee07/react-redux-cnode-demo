.deliver-page-wrapper
	height 100%
	background-color #fff
	.deliver-page-header
		position relative
		background-color rgba(51,51,51,.78)
		width 100%
		height 60px
		line-height 60px
		text-align center
		font-size 16px
		color #fdf4da
	.deliver-page-content
		text-align center
		font-size 14px
		padding 20px
		.deliver-type-select
			margin-bottom 20px
			label
				margin-right 10px
			select
				width 80px
				height 28px
				background-color #fff
				border-radius 5px
		.deliver-title-input
			margin-bottom 20px
			input
				width 100%
				height 28px
				background-color #fff
				border-radius 5px
				border 1px solid #eeeeee
		.deliver-content-textarea
			margin-bottom 20px
			textarea
				width 100%
				height 200px
				resize none
				background-color #fff
				border-radius 5px
				border 1px solid #eeeeee
		.deliver-btn
			width 80px
			height 28px
			color #fff
			background-color #108ee9
			border-radius 5px
			border none